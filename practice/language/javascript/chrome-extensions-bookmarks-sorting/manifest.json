{
	"name": "Bookmarks Sorting",
	"description": "Extension to sort bookmarks..",
	"version": "1.0",
	"manifest_version": 3,
	"permissions": ["bookmarks", "storage"],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_title": "Bookmarks Sorting",
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	}
}
